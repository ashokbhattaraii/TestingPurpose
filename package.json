{
    "name": "omus",
    "version": "1.0.0",
    "private": true,
    "description": "OMUS â€” Monorepo powered by Turborepo (Next.js + NestJS)",
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "dev": "turbo run dev",
        "dev:web": "turbo run dev --filter=@omus/web",
        "dev:api": "turbo run dev --filter=@omus/api",
        "build": "turbo run build",
        "build:web": "turbo run build --filter=@omus/web",
        "build:api": "turbo run build --filter=@omus/api",
        "start": "turbo run start",
        "start:web": "turbo run start --filter=@omus/web",
        "start:api": "turbo run start --filter=@omus/api",
        "lint": "turbo run lint",
        "lint:web": "turbo run lint --filter=@omus/web",
        "lint:api": "turbo run lint --filter=@omus/api",
        "format": "turbo run format",
        "format:api": "turbo run format --filter=@omus/api",
        "test": "turbo run test",
        "test:web": "turbo run test --filter=@omus/web",
        "test:api": "turbo run test --filter=@omus/api",
        "test:e2e": "turbo run test:e2e --filter=@omus/api",
        "clean": "turbo run clean",
        "clean:web": "turbo run clean --filter=@omus/web",
        "clean:api": "turbo run clean --filter=@omus/api"
    },
    "devDependencies": {
        "turbo": "^2.3.3"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=10.0.0"
    },
    "packageManager": "npm@10.0.0"
}