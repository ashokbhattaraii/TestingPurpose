{
    "$schema": "https://turbo.build/schema.json",
    "ui": "stream",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "inputs": [
                "$TURBO_DEFAULT$",
                ".env*"
            ],
            "outputs": [
                ".next/**",
                "!.next/cache/**",
                "dist/**"
            ]
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "start": {
            "dependsOn": [
                "build"
            ],
            "cache": false,
            "persistent": true
        },
        "lint": {
            "dependsOn": [
                "^lint"
            ],
            "outputs": []
        },
        "format": {
            "cache": false,
            "outputs": []
        },
        "test": {
            "dependsOn": [
                "^build"
            ],
            "inputs": [
                "$TURBO_DEFAULT$",
                ".env*"
            ],
            "outputs": [
                "coverage/**"
            ]
        },
        "test:e2e": {
            "dependsOn": [
                "^build"
            ],
            "cache": false,
            "inputs": [
                "$TURBO_DEFAULT$",
                ".env*"
            ]
        },
        "clean": {
            "cache": false
        }
    }
}